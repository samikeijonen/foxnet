  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "editor"
  ],
  "homepage": "https://github.com/samikeijonen/foxnet",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/samikeijonen/foxnet"
  },
  "bugs": "https://github.com/samikeijonen/foxnet/issues",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@meom/navigation": "^1.0.5",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@meom/scripts": "^4.0.0"
  },
  "scripts": {
    "build": "wp-scripts build --webpack-no-externals",
    "check-engines": "wp-scripts check-engines",
    "format:js": "wp-scripts format 'htdocs/wp-content/themes/aisti/js/**/*.js'",
    "format-blocks:js": "wp-scripts format 'htdocs/wp-content/plugins/meomblocks/**/*.js'",
    "lint": "npm run lint:css && npm run lint-blocks:css && npm run lint:js && npm run lint-blocks:js",
    "lint:css": "wp-scripts lint-style 'htdocs/wp-content/themes/aisti/scss/**/*.scss'",
    "lint-blocks:css": "wp-scripts lint-style 'htdocs/wp-content/plugins/meomblocks/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'htdocs/wp-content/themes/aisti/js/**/*.js'",
    "lint-blocks:js": "wp-scripts lint-js 'htdocs/wp-content/plugins/meomblocks/**/*.js'",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "start": "wp-scripts start --webpack-no-externals",
    "test": "scripts/run-tests"
  }
}